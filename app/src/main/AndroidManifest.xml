<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.kyozen.testsensor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SensorAccActivity" />
        <activity android:name=".SensorGyroActivity" />
        <activity android:name=".SensorMagnetActivity" />
        <activity android:name=".SensorLightActivity" />
        <activity android:name=".SensorPressureActivity" />
        <activity android:name=".SensorTempActivity" />
        <activity android:name=".SensorHumdityActivity" />
        <activity android:name=".SensorProxActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SensorOrientationActivity" />
        <activity android:name=".AboutActivity"></activity>
    </application>

</manifest>